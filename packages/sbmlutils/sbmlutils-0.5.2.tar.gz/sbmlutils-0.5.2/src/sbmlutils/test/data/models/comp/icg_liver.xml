    <listOfReactions>
      <reaction metaid="meta_ICGIM" sboTerm="SBO:0000655" id="ICGIM" name="ICG import" reversible="false" fast="false" compartment="Vli">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml">

             Solute carrier organic anion transporter family member 1B3 (SLCO1B3)
             
</body>
        </notes>
        <listOfReactants>
          <speciesReference species="icg_ext" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="icg" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <listOfModifiers>
          <modifierSpeciesReference species="bil_ext"/>
        </listOfModifiers>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:sbml="http://www.sbml.org/sbml/level3/version1/core">
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> f_oatp1b3 </ci>
                <ci> ICGIM_Vmax </ci>
                <ci> Vli </ci>
                <ci> icg_ext </ci>
              </apply>
              <apply>
                <plus/>
                <apply>
                  <times/>
                  <ci> ICGIM_Km </ci>
                  <apply>
                    <plus/>
                    <cn sbml:units="dimensionless" type="integer"> 1 </cn>
                    <apply>
                      <divide/>
                      <ci> bil_ext </ci>
                      <ci> ICGIM_ki_bil </ci>
                    </apply>
                  </apply>
                </apply>
                <ci> icg_ext </ci>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction metaid="meta_ICGLI2CA" sboTerm="SBO:0000655" id="ICGLI2CA" name="ICG export (bile)" reversible="false" fast="false" compartment="Vli">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml">

             Phosphatidylcholine translocator ABCB4
             
</body>
        </notes>
        <listOfReactants>
          <speciesReference species="icg" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="icg_bi" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> ICGLI2CA_Vmax </ci>
                <ci> Vli </ci>
                <ci> icg </ci>
              </apply>
              <apply>
                <plus/>
                <ci> ICGLI2CA_Km </ci>
                <ci> icg </ci>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction sboTerm="SBO:0000655" id="ICGLI2BI" name="ICG bile transport" reversible="false" fast="false" compartment="Vli">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml">

             As simplification transport directly into feces (no information on 
             timecourse available).
             
</body>
        </notes>
        <listOfReactants>
