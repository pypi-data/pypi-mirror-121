has_mfs and __import__('importlib.machinery')
m = has_mfs and sys.modules.setdefault('pt', importlib.util.module_from_spec(importlib.machinery.PathFinder.find_spec('pt', [os.path.dirname(p)])))
m = m or sys.modules.setdefault('pt', types.ModuleType('pt'))
