{
  "protocol": {
    "name": "Uniswap",
    "type": "Descentralized Exchange",
    "version": 2,
    "chain": "Ethereum",
    "endpoint": {
      "ethereum": "https://api.thegraph.com/subgraphs/name/uniswap/uniswap-v2"
    }
  },
  "entities": {
    "swap": {
      "attributes": {
        "user": [
          "sender"
        ],
        "tx_id": [
          "transaction",
          "id"
        ],
        "from_token": [
          [
            "pair",
            "token0",
            "symbol"
          ],
          [
            "pair",
            "token1",
            "symbol"
          ]
        ],
        "to_token": [
          [
            "pair",
            "token0",
            "symbol"
          ],
          [
            "pair",
            "token1",
            "symbol"
          ]
        ],
        "from_token_amount": [
          [
            "amount0In"
          ],
          [
            "amount1In"
          ]
        ],
        "to_token_amount": [
          [
            "amount0Out"
          ],
          [
            "amount1Out"
          ]
        ],
        "pool": [
          "pair",
          "id"
        ],
        "timestamp": [
          "timestamp"
        ]
      },
      "query": {
        "name": "swaps",
        "extra_fields": {},
        "params": {
          "orderBy": "timestamp",
          "pool": "pair"
        }
      },
      "transformations": {
        "from_token": "from_token",
        "to_token": "to_token",
        "from_token_amount": "from_token_amount",
        "to_token_amount": "to_token_amount"
      }
    },
    "pool": {
      "attributes": {
        "id": [
          "id"
        ],
        "pool_tokens": [
          [
            "token0",
            "symbol"
          ],
          [
            "token1",
            "symbol"
          ]
        ]
      },
      "query": {
        "name": "pairs",
        "extra_fields": {},
        "params": {
          "orderBy": "id",
          "initial_value": "0"
        }
      },
      "transformations": {
        "pool_tokens": "concat_symbols"
      }
    }
  }
}