#!/home/johnphan/pypi/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'geneflow3==3.0.0a1','console_scripts','gf'
__requires__ = 'geneflow3==3.0.0a1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('geneflow3==3.0.0a1', 'console_scripts', 'gf')()
    )
